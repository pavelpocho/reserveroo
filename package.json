{
  "name": "remix-template-netlify",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production netlify build",
    "dev": "npm run clean && cross-env NODE_ENV=development netlify dev",
    "start": "cross-env NODE_ENV=production netlify dev",
    "clean": "node clean.js",
    "migrate-prisma": "prisma migrate dev && prisma db seed",
    "deploy": "npm run build && netlify deploy"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@prisma/client": "^3.13.0",
    "@remix-run/netlify": "^1.4.3",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.5",
    "tiny-invariant": "^1.2.0",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.25",
    "bcrypt": "^5.0.1",
    "eslint": "^8.11.0",
    "prisma": "^3.13.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
